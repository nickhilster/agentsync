{
  "name": "agentsync",
  "displayName": "AgentSync",
  "description": "Multi-agent coordination protocol for VS Code. Sync Claude, Codex, and Copilot on any project.",
  "version": "0.3.4",
  "publisher": "Teambotics",
  "icon": "logo_icon/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickhilster/agentsync.git"
  },
  "bugs": {
    "url": "https://github.com/nickhilster/agentsync/issues"
  },
  "homepage": "https://github.com/nickhilster/agentsync",
  "license": "MIT",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#0d1117",
    "theme": "dark"
  },
  "keywords": [
    "ai",
    "agents",
    "claude",
    "codex",
    "copilot",
    "coordination",
    "handoff"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "scripts": {
    "vsix:package": "npx --yes @vscode/vsce package --out agentsync-local.vsix",
    "vsix:install": "node scripts/refresh-vsix.js --skip-package",
    "vsix:refresh": "node scripts/refresh-vsix.js",
    "lint": "eslint extension.js scripts/",
    "lint:fix": "eslint extension.js scripts/ --fix",
    "format": "prettier --write extension.js scripts/",
    "format:check": "prettier --check extension.js scripts/",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "bump:patch": "node scripts/bump-version.js patch",
    "bump:minor": "node scripts/bump-version.js minor",
    "bump:major": "node scripts/bump-version.js major"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "jest": "^29.7.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "contributes": {
    "commands": [
      {
        "command": "agentsync.init",
        "title": "AgentSync: Initialize Workspace"
      },
      {
        "command": "agentsync.openTracker",
        "title": "AgentSync: Open AgentTracker",
        "icon": "$(book)"
      },
      {
        "command": "agentsync.openPanel",
        "title": "AgentSync: Open Panel",
        "icon": "$(layout-sidebar-left)"
      },
      {
        "command": "agentsync.openDashboard",
        "title": "AgentSync: Open Live Dashboard",
        "icon": "$(pulse)"
      },
      {
        "command": "agentsync.openHandoffs",
        "title": "AgentSync: Open Handoffs JSON",
        "icon": "$(json)"
      },
      {
        "command": "agentsync.clearActiveSession",
        "title": "AgentSync: Clear Active Session",
        "icon": "$(circle-slash)"
      },
      {
        "command": "agentsync.startSession",
        "title": "AgentSync: Start Session",
        "icon": "$(play)"
      },
      {
        "command": "agentsync.endSession",
        "title": "AgentSync: End Session",
        "icon": "$(debug-stop)"
      },
      {
        "command": "agentsync.detectCommands",
        "title": "AgentSync: Detect Build/Test Commands"
      },
      {
        "command": "agentsync.refreshPanel",
        "title": "AgentSync: Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "agentsync.openTutorial",
        "title": "AgentSync: Open Interactive Tutorial",
        "icon": "$(mortar-board)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agentsync",
          "title": "AgentSync",
          "icon": "logo_icon/agentsync-icon.svg"
        }
      ]
    },
    "views": {
      "agentsync": [
        {
          "type": "webview",
          "id": "agentsync.dashboard",
          "name": "AgentSync Live",
          "contextualTitle": "AgentSync Live"
        },
        {
          "id": "agentsync.panel",
          "name": "AgentSync Details",
          "contextualTitle": "AgentSync Details"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "agentsync.startSession",
          "when": "view == agentsync.panel || view == agentsync.dashboard",
          "group": "navigation@1"
        },
        {
          "command": "agentsync.endSession",
          "when": "view == agentsync.panel || view == agentsync.dashboard",
          "group": "navigation@2"
        },
        {
          "command": "agentsync.clearActiveSession",
          "when": "view == agentsync.panel || view == agentsync.dashboard",
          "group": "navigation@3"
        },
        {
          "command": "agentsync.openTracker",
          "when": "view == agentsync.panel || view == agentsync.dashboard",
          "group": "navigation@4"
        },
        {
          "command": "agentsync.openHandoffs",
          "when": "view == agentsync.panel || view == agentsync.dashboard",
          "group": "navigation@5"
        },
        {
          "command": "agentsync.refreshPanel",
          "when": "view == agentsync.panel || view == agentsync.dashboard",
          "group": "navigation@6"
        },
        {
          "command": "agentsync.openTutorial",
          "when": "view == agentsync.panel || view == agentsync.dashboard",
          "group": "navigation@7"
        }
      ]
    },
    "walkthroughs": [
      {
        "id": "agentsync.gettingStarted",
        "title": "AgentSync Interactive Tutorial",
        "description": "A guided setup for new users to initialize AgentSync, run a session, and close it with clear handoff context.",
        "steps": [
          {
            "id": "agentsync.tutorial.openDashboard",
            "title": "Open AgentSync Live",
            "description": "Open the live dashboard with [AgentSync: Open Live Dashboard](command:agentsync.openDashboard).",
            "completionEvents": [
              "onCommand:agentsync.openDashboard"
            ]
          },
          {
            "id": "agentsync.tutorial.initialize",
            "title": "Initialize your workspace",
            "description": "Create the coordination files with [AgentSync: Initialize Workspace](command:agentsync.init).",
            "completionEvents": [
              "onCommand:agentsync.init"
            ]
          },
          {
            "id": "agentsync.tutorial.startSession",
            "title": "Start a session",
            "description": "Begin active work with [AgentSync: Start Session](command:agentsync.startSession).",
            "completionEvents": [
              "onCommand:agentsync.startSession"
            ]
          },
          {
            "id": "agentsync.tutorial.endSession",
            "title": "End and hand off",
            "description": "Finish with [AgentSync: End Session](command:agentsync.endSession) to update health, hot files, and next work.",
            "completionEvents": [
              "onCommand:agentsync.endSession"
            ]
          }
        ]
      }
    ],
    "configuration": {
      "title": "AgentSync",
      "properties": {
        "agentsync.promptOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "Show a notification when VS Code opens and an AgentSync session is already active."
        },
        "agentsync.sessionReminderHours": {
          "type": "number",
          "default": 2,
          "minimum": 0,
          "description": "Show a reminder notification after a session has been active for this many hours. Set to 0 to disable."
        },
        "agentsync.autoDetectCommands": {
          "type": "boolean",
          "default": true,
          "description": "During workspace initialization, automatically detect npm scripts and offer to populate .agentsync.json commands."
        },
        "agentsync.autoStaleSessionMinutes": {
          "type": "number",
          "default": 0,
          "minimum": 0,
          "description": "Treat active sessions older than this many minutes as stale (UI shows Waiting instead of Busy). Set to 0 to disable."
        }
      }
    }
  }
}
